{
    "name": "vite-tpl",
    "version": "0.1.0",
    "description": "vite tpl",
    "main": "index.js",
    "scripts": {
        "dev": "vite --open",
        "serve": "pnpm dev",
        "build": "vite build",
        "preview": "vite preview",
        "preview:build": "pnpm build && vite preview",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint",
        "lint:eslint": "eslint \"src/**/*.{vue,ts,js}\" --fix",
        "lint:stylelint": "stylelint \"src/**/*.{vue,html,css,less,scss,sass}\" --fix",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install"
    },
    "keywords": [
        "vite",
        "vue"
    ],
    "author": "zhaoqing",
    "license": "MIT",
    "dependencies": {
        "@vueuse/components": "^9.6.0",
        "@vueuse/core": "^9.6.0",
        "axios": "^1.2.1",
        "element-plus": "^2.2.26",
        "pinia": "^2.0.27",
        "qs": "^6.11.0",
        "tp-common.css": "^1.0.2",
        "vue": "^3.2.45",
        "vue-router": "^4.1.6"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.3.0",
        "@commitlint/config-conventional": "^17.3.0",
        "@rollup/plugin-strip": "^3.0.1",
        "@trscd/eslint-config": "^0.1.4",
        "@trscd/stylelint-config-tpconfig": "^1.1.3",
        "@types/node": "^18.11.11",
        "@types/qs": "^6.9.7",
        "@typescript-eslint/parser": "^5.45.1",
        "@unocss/transformer-directives": "^0.47.5",
        "@vitejs/plugin-vue": "^3.2.0",
        "@vue/compiler-sfc": "^3.2.45",
        "@vue/eslint-config-typescript": "^11.0.2",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.29.0",
        "eslint-plugin-vue": "^9.8.0",
        "husky": "^8.0.2",
        "less": "^4.1.3",
        "lint-staged": "^13.1.0",
        "postcss": "^8.4.19",
        "stylelint": "^14.16.0",
        "typescript": "^4.9.4",
        "unocss": "^0.47.5",
        "unplugin-auto-import": "^0.12.0",
        "unplugin-vue-components": "^0.22.11",
        "vite": "^3.2.5",
        "vite-plugin-import-icons": "^0.0.9"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts,vue}": "pnpm lint:eslint",
        "*.{vue,html,css,less,scss,sass}": "pnpm lint:stylelint"
    }
}
