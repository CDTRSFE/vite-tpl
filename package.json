    },
    "devDependencies": {
        "@rushstack/eslint-patch": "^1.1.0",
        "@trscd/eslint-config": "^0.1.1",
        "@types/node": "^17.0.21",
        "@typescript-eslint/parser": "^5.13.0",
        "@vitejs/plugin-vue": "^2.2.4",
        "@vue/compiler-sfc": "^3.2.31",
        "@vue/eslint-config-typescript": "^10.0.0",
        "cz-conventional-changelog": "^3.3.0",
