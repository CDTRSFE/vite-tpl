{
    "name": "vite-tpl",
    "version": "0.1.0",
    "description": "vite tpl",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "pnpm dev",
        "build": "vite build",
        "preview": "vite preview",
        "preview:build": "pnpm build && vite preview",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint",
        "lint:eslint": "eslint \"src/**/*.{vue,ts,js}\"",
        "lint:stylelint": "stylelint \"src/**/*.{vue,html,css,less,scss,sass}\" --fix",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install"
    },
    "keywords": [
        "vite",
        "vue"
    ],
    "tagPrefix": [
        "wx-dev-v"
    ],
    "tag": "0.1.0",
    "author": "zhaoqing",
    "license": "MIT",
    "dependencies": {
        "@vueuse/components": "^13.0.0",
        "@vueuse/core": "^13.0.0",
        "ant-design-vue": "^4.2.6",
        "axios": "^1.8.4",
        "lodash": "^4.17.21",
        "pinia": "^3.0.1",
        "qs": "^6.14.0",
        "tp-common.css": "^1.0.3",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/eslintrc": "^3.3.0",
        "@rollup/plugin-strip": "^3.0.4",
        "@trscd/stylelint-config-tpconfig": "^1.1.4",
        "@types/lodash": "^4.17.16",
        "@types/node": "^22.13.10",
        "@types/qs": "^6.9.18",
        "@typescript-eslint/parser": "^8.27.0",
        "@unocss/transformer-directives": "66.1.0-beta.6",
        "@vitejs/plugin-vue": "^5.2.3",
        "@vitejs/plugin-vue-jsx": "^4.1.2",
        "@vue/compiler-sfc": "^3.5.13",
        "@vue/eslint-config-typescript": "^14.5.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^9.22.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-unicorn": "^57.0.0",
        "eslint-plugin-vue": "^10.0.0",
        "globals": "^16.0.0",
        "husky": "^9.1.7",
        "less": "^4.2.2",
        "lint-staged": "^15.5.0",
        "postcss": "^8.5.3",
        "stylelint": "^16.16.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.27.0",
        "unocss": "66.1.0-beta.6",
        "unplugin-auto-import": "^19.1.1",
        "unplugin-vue-components": "^28.4.1",
        "vite": "^6.2.2",
        "vite-plugin-import-icons": "^1.0.4",
        "vite-plugin-zip-pack": "^1.2.4",
        "vue-eslint-parser": "^10.1.1"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts,vue}": "pnpm lint:eslint",
        "*.{vue,html,css,less,scss,sass}": "pnpm lint:stylelint"
    }
}
