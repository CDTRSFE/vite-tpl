{
    "name": "vite-tpl",
    "version": "0.1.0",
    "description": "vite tpl",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "serve": "pnpm dev",
        "build": "vite build",
        "preview": "vite preview",
        "preview:build": "pnpm build && vite preview",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint",
        "lint:eslint": "eslint \"src/**/*.{vue,ts,js}\" --fix",
        "lint:stylelint": "stylelint \"src/**/*.{vue,html,css,less,scss,sass}\" --fix",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install"
    },
    "keywords": [
        "vite",
        "vue"
    ],
    "tagPrefix": [
        "wx-dev-v"
    ],
    "tag": "0.1.0",
    "author": "zhaoqing",
    "license": "MIT",
    "dependencies": {
        "@vueuse/components": "^10.9.0",
        "@vueuse/core": "^10.9.0",
        "ant-design-vue": "^4.1.2",
        "axios": "^1.6.8",
        "lodash": "^4.17.21",
        "pinia": "^2.1.7",
        "qs": "^6.12.0",
        "tp-common.css": "^1.0.3",
        "vue": "^3.4.21",
        "vue-router": "^4.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@rollup/plugin-strip": "^3.0.4",
        "@trscd/eslint-config": "^0.1.4",
        "@trscd/stylelint-config-tpconfig": "^1.1.3",
        "@types/lodash": "^4.17.0",
        "@types/node": "^20.12.2",
        "@types/qs": "^6.9.14",
        "@typescript-eslint/parser": "^7.5.0",
        "@unocss/transformer-directives": "0.59.0-beta.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitejs/plugin-vue-jsx": "^3.1.0",
        "@vue/compiler-sfc": "^3.4.21",
        "@vue/eslint-config-typescript": "^13.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.0",
        "eslint-plugin-vue": "^9.24.0",
        "husky": "^9.0.11",
        "less": "^4.2.0",
        "lint-staged": "^15.2.2",
        "postcss": "^8.4.38",
        "stylelint": "^15.10.3",
        "typescript": "^5.4.3",
        "unocss": "0.59.0-beta.1",
        "unplugin-auto-import": "^0.17.5",
        "unplugin-vue-components": "^0.26.0",
        "vite": "^5.2.7",
        "vite-plugin-import-icons": "^0.0.11",
        "vite-plugin-zip-pack": "^1.2.2"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts,vue}": "pnpm lint:eslint",
        "*.{vue,html,css,less,scss,sass}": "pnpm lint:stylelint"
    }
}
