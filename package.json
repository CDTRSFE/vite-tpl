{
    "name": "vite-tpl",
    "version": "0.1.0",
    "description": "vite tpl",
    "main": "index.js",
    "scripts": {
        "dev": "vite --open",
        "serve": "pnpm dev",
        "build": "vite build",
        "preview": "vite preview",
        "preview:build": "pnpm build && vite preview",
        "lint": "pnpm lint:eslint && pnpm lint:stylelint",
        "lint:eslint": "eslint \"src/**/*.{vue,ts,js}\" --fix",
        "lint:stylelint": "stylelint \"src/**/*.{vue,html,css,less,scss,sass}\" --fix",
        "preinstall": "npx only-allow pnpm",
        "prepare": "husky install"
    },
    "keywords": [
        "vite",
        "vue"
    ],
    "author": "zhaoqing",
    "license": "MIT",
    "dependencies": {
        "@vueuse/components": "^10.2.1",
        "@vueuse/core": "^10.2.1",
        "ant-design-vue": "^4.0.3",
        "axios": "^1.4.0",
        "lodash": "^4.17.21",
        "pinia": "^2.1.4",
        "qs": "^6.11.2",
        "tp-common.css": "^1.0.3",
        "vue": "^3.3.4",
        "vue-router": "^4.2.2"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.6",
        "@commitlint/config-conventional": "^17.6.6",
        "@rollup/plugin-strip": "^3.0.2",
        "@trscd/eslint-config": "^0.1.4",
        "@trscd/stylelint-config-tpconfig": "^1.1.3",
        "@types/lodash": "^4.14.195",
        "@types/node": "^20.3.3",
        "@types/qs": "^6.9.7",
        "@typescript-eslint/parser": "^5.61.0",
        "@unocss/transformer-directives": "^0.53.4",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "@vue/compiler-sfc": "^3.3.4",
        "@vue/eslint-config-typescript": "^11.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.44.0",
        "eslint-plugin-vue": "^9.15.1",
        "husky": "^8.0.3",
        "less": "^4.1.3",
        "lint-staged": "^13.2.3",
        "postcss": "^8.4.24",
        "stylelint": "^15.9.0",
        "typescript": "^5.1.6",
        "unocss": "^0.53.4",
        "unplugin-auto-import": "^0.16.0",
        "unplugin-vue-components": "^0.25.2",
        "vite": "^4.3.9",
        "vite-plugin-import-icons": "^0.0.10"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog",
            "disableScopeLowerCase": true,
            "disableSubjectLowerCase": true
        }
    },
    "lint-staged": {
        "*.{js,ts,vue}": "pnpm lint:eslint",
        "*.{vue,html,css,less,scss,sass}": "pnpm lint:stylelint"
    }
}
